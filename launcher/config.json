{
    "resources": [ {"port_range": [9001, 9050], "host": "localhost"} ],
    "sessionData": {
      "updir": "/Home"
    },
    "configuration": {
      "host": "localhost",
      "port": 9000,
      "endpoint": "paraview",
      "log_dir": "/pvw/launcher/log",
      "sessionURL": "SESSION_URL_ROOT/proxy?sessionId=${id}&path=ws",
      "timeout": 25,
      "upload_dir": "/data/upload",
      "fields": [],
      "proxy_file": "/opt/launcher/proxy-mapping.txt",
      "sanitize": {
        "version": {
            "type": "regexp",
            "regexp": "^v[0-9]+.[0-9]+.[0-9]+$",
            "default": "v0.0.0"
        },
        "file": {
            "type": "regexp",
            "regexp": "^[-\\\\w./]+$",
            "default": "emptyFile"
        }
      }
    },
    "properties": {
      "dataDir": "/data",
      "webapps_dir": "/opt/paraview/share/paraview-5.7/web",
      "python_exec": "/opt/paraview/bin/pvpython"
    },
    "apps": {
      "visualizer": {
        "cmd": [
          "${python_exec}",
          EXTRA_PVPYTHON_ARGS
          "${webapps_dir}/visualizer/server/pvw-visualizer.py",
          "--port", "${port}",
          "--data", "${dataDir}",
          "--authKey", "${secret}",
          "--viewport-max-width", "1920",
          "--viewport-max-height", "1080",
          "--timeout", "30"
        ],
        "ready_line" : " Starting factory"
      },
      "visualizer-with-file": {
        "cmd": [
          "${python_exec}",
          EXTRA_PVPYTHON_ARGS
          "${webapps_dir}/visualizer/server/pvw-visualizer.py",
          "--port", "${port}",
          "--data", "${dataDir}",
          "--authKey", "${secret}",
          "--viewport-max-width", "1920",
          "--viewport-max-height", "1080",
          "--timeout", "30",
          "--load-file", "${file}"
        ],
        "ready_line" : " Starting factory"
      },
      "paraview-lite": {
        "cmd": [
          "${python_exec}",
          EXTRA_PVPYTHON_ARGS
          "${webapps_dir}/lite/server/pvw-lite.py",
          "--port", "${port}",
          "--data", "${dataDir}",
          "--authKey", "${secret}",
          "--viewport-max-width", "1920",
          "--viewport-max-height", "1080",
          "--timeout", "30"
        ],
        "ready_line" : " Starting factory"
      },
      "divvy": {
        "cmd": [
          "${python_exec}",
          EXTRA_PVPYTHON_ARGS
          "${webapps_dir}/divvy/server/pvw-divvy.py",
          "--port", "${port}",
          "--data", "${dataDir}/${file}",
          "--authKey", "${secret}",
          "--viewport-max-width", "1920",
          "--viewport-max-height", "1080",
          "--timeout", "30"
        ],
        "ready_line" : " Starting factory"
      },
      "flow": {
        "cmd": [
          "${python_exec}",
          EXTRA_PVPYTHON_ARGS
          "${webapps_dir}/flow/server/pvw-flow.py",
          "--port", "${port}",
          "--data", "${dataDir}/${file}",
          "--authKey", "${secret}",
          "--viewport-max-width", "1920",
          "--viewport-max-height", "1080",
          "--timeout", "30"
        ],
        "ready_line" : " Starting factory"
      },
      "fury": {
        "cmd": [
          "${python_exec}",
          EXTRA_PVPYTHON_ARGS
          "/pvw/apps/fury/server/fury_server.py",
          "--port", "${port}",
          "--authKey", "${secret}"
        ],
        "ready_line" : " Starting factory"
      },
      "demo": {
        "cmd": [
          "${python_exec}",
          EXTRA_PVPYTHON_ARGS
          "/pvw/apps/demo/server/vtk_server.py",
          "--port", "${port}",
          "--authKey", "${secret}"
        ],
        "ready_line" : " Starting factory"
      },
      "sdf": {
        "cmd": [
          "${python_exec}",
          EXTRA_PVPYTHON_ARGS
          "/pvw/apps/sdf/server/vtk_server.py",
          "--port", "${port}",
          "--authKey", "${secret}",
          "--load-centers", "${centers}",
          "--load-sims", "${sims}"
        ],
        "ready_line" : " Starting factory"
      },
      "spheres": {
        "cmd": [
          "${python_exec}",
          EXTRA_PVPYTHON_ARGS
          "/pvw/apps/spheres/server/fury_server.py",
          "--port", "${port}",
          "--authKey", "${secret}"
        ],
        "ready_line" : " Starting factory"
      },
      "tumor": {
        "cmd": [
          "${python_exec}",
          EXTRA_PVPYTHON_ARGS
          "/pvw/apps/tumor/server/fury_server.py",
          "--port", "${port}",
          "--authKey", "${secret}"
        ],
        "ready_line" : " Starting factory"
      }
    }
  }